 267  getargs :: TiHeap -> TiStack -> [Addr]
 268  getargs heap stack = case pop stack of
 269      (sc, stack1) -> map getarg stack1.stkItems
 270          where
 271              getarg addr = case hLookup heap addr of
 272                  NAp _ arg -> arg
 273                  _         -> error "getarg: not application node"
 274  
